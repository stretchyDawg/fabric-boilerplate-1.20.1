		"main": [
			"net.stretchydawg.demomod.DemoMod"
		],
		"client": [
			"net.stretchydawg.demomod.DemoModClient"
		],
		"fabric-datagen": [
			"net.stretchydawg.demomod.DemoModDataGenerator"
		]
	},
	"mixins": [
		"demomod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.7",
		"minecraft": "~1.20.1",
		"java": ">=17",
